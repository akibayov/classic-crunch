{
    "sourceMaps": "inline",
    "presets": [
        [
            "@babel/env",
            {
                "targets": {"node": "current"},
                "useBuiltIns": "usage",
                "corejs": {"version": 3, "proposals": false},
                "modules": false
            }
        ],
        "@babel/typescript"
    ],
    "plugins": [
        ["module-resolver", {"alias": {"classic-crunch": "./src"}}]
    ]
}
